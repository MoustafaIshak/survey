<aura:component implements="flexipage:availableForAllPageTypes,force:lightningQuickAction,flexipage:availableForRecordHome,force:hasRecordId" access="global" controller='caseWidgetController' >
	<aura:attribute name="recordId" type="String" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <lightning:spinner aura:id="loadingProgress" variant="brand" size="large" /> 
	<aura:attribute name="costPerCalnedarMonth" type="string" />
    <aura:attribute name="otherCountryAtmFee" type="string" />
    <aura:attribute name="cardReplacementCost" type="string" />
    
    <p>{!v.costPerCalnedarMonth}  </p>
    <p>{!v.otherCountryAtmFee}  </p>
    <p>{!v.cardReplacementCost}  </p>
</aura:component>